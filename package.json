    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-angular": "^12.0.1",
    "eslint": "^7.23.0",

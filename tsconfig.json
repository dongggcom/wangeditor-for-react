{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "esnext",
      "dom"
    ],
    "outDir": "lib",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react"
  },
  "include": [
    "ts"
  ],
  "exclude": [
    "es6",
    "script",
    "node_modules",
    "lib"
  ]
}